USE_JACKSON_PARAM = False       !   [Boolean] default = False
                                ! If true, use the Jackson-Hallberg-Legg (JPO 2008) shear mixing
                                ! parameterization.
TKE_BACKGROUND = 0.0            !   [m2 s-2] default = 0.0
